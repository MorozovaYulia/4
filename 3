#include <iostream> 
using namespace std; 
int main() 
{ 
    int a, x, y; 
    cout<<"vvedite kolichestvo kg: "; 
    cin>>x; 
    cout<<"vvedite stoimost vseh kg: "; 
    cin>>a; 
    cout<<"vvedite novoe kolichestvo kg: "; 
    cin>>y; 
    cout<< "1 kg stoit "<<a/x<<"rub"<<endl; 
    cout<< y<< " kg stoit "<<y*(a/x)<<"rub"<<endl; 
} 
