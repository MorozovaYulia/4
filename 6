#include <iostream> 
using namespace std; 
int main()
{ 
    int a1, b1, a2, b2, c1, c2, det, x, y; 
    cout<<" reshenie sistem uravnenii vida a1x+b1y=c1 Ğ¸ a2x+b2y=c2:"<<endl; 
    cout<<" vvedite a1: "; 
    cin>>a1; 
    cout<<"\n vvedite b1: "; 
    cin>>b1; 
    cout<<"\n vvedite c1: "; 
    cin>>c1; 
    cout<<"\n vvedite a2: "; 
    cin>>a2; 
    cout<<"\n vvedite b2: "; 
    cin>>b2; 
    cout<<"\n vvedite c2: "; 
    cin>>c2; 
    det=a1*b2-a2*b1; 
    x=(c1*b2-b1*c2)/det; 
    y=(c2*a1-c1*a2)/det; 
    cout<< "\n x = "<<x; 
    cout<< "\n y = "<<y; 
} 
