#include <iostream> 
using namespace std; 
int main()
{ 
    int a1, b1, a2, b2, c1, c2, d, x, y;  
    cout<<"Enter a1: \n"; 
    cin>>a1; 
    cout<<"Enter b1: \n"; 
    cin>>b1; 
    cout<<"Enter c1: \n"; 
    cin>>c1; 
    cout<<"Enter a2: \n"; 
    cin>>a2; 
    cout<<"Enter b2: \n"; 
    cin>>b2; 
    cout<<"Enter c2: \n"; 
    cin>>c2; 
    d=a1*b2-a2*b1; 
    x=(c1*b2-b1*c2)/d; 
    y=(c2*a1-c1*a2)/d; 
    cout<< "x = "<<x<<"\n"; 
    cout<< "y = "<<y; 
} 
